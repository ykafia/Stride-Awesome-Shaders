namespace Stride.Noises
{
    shader PerlinCompute2D<float Scale> : 
        ComputeColor, Texturing, PerlinNoise2D<Scale>
    {
        override float4 Compute()
        {
            float v = ComputeNoise(streams.TexCoord);
            return saturate(float4(v,v,v,v));
        }

    };
        
}

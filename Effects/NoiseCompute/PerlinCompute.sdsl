namespace Stride.Noises
{
    shader PerlinCompute : ComputeColor, PositionStream4,PerlinNoise
    {
        override float4 Compute()
        {
            float v = ComputeNoise(streams.Position.xyz);
            //if(v<0.6) v = 0;
            return saturate(float4(v,v,v,v));
        }

    };
        
}
